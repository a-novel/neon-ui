{
  "name": "@a-novel/neon-ui",
  "private": false,
  "version": "2.6.8",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint:stylecheck": "prettier --check . --config prettier.config.ts",
    "lint:typecheck": "tsc",
    "lint:eslint": "pnpm dlx eslint .",
    "lint": "pnpm lint:stylecheck && pnpm lint:typecheck && pnpm lint:eslint",
    "format": "prettier --write . --config prettier.config.ts",
    "build": "rm -rf ./dist && vite build && tsc --build tsconfig.build.json",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "A-Novel",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-novel/neon-ui.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui/ui.es.js",
      "types": "./dist/ui/index.d.ts"
    },
    "./ux": {
      "import": "./dist/ux/ux.es.js",
      "types": "./dist/ux/index.d.ts"
    },
    "./storybook": {
      "import": "./dist/storybook/storybook.es.js",
      "types": "./dist/storybook/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@fontsource/arimo": "^5.2.6",
    "@fontsource/bungee": "^5.2.6",
    "i18next": "^25.2.1",
    "react-i18next": "^15.5.2"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "@tanstack/react-form": "^1.11.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@storybook/addon-docs": "9.1.0-alpha.3",
    "@storybook/addon-onboarding": "9.1.0-alpha.3",
    "@storybook/addon-themes": "^9.1.0-alpha.3",
    "@storybook/react-vite": "^9.1.0-alpha.3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.7",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.1.0-alpha.3",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "storybook": "^9.1.0-alpha.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "yaml": "^2.8.0"
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "npm": ">=10.9.2",
    "node": ">=23.11.0"
  }
}
