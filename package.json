{
  "name": "@a-novel/neon-ui",
  "version": "2.8.5",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/a-novel/neon-ui.git"
  },
  "license": "AGPL-3.0",
  "author": "A-Novel",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.es.js",
      "types": "./dist/src/index.d.ts"
    },
    "./ui": {
      "import": "./dist/src/ui/index.es.js",
      "types": "./dist/src/ui/index.d.ts"
    },
    "./ux": {
      "import": "./dist/src/ux/index.es.js",
      "types": "./dist/src/ux/index.d.ts"
    },
    "./storybook": {
      "import": "./dist/src/storybook/index.es.js",
      "types": "./dist/src/storybook/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf ./dist && vite build && tsc --build tsconfig.build.json",
    "build-storybook": "storybook build",
    "format": "prettier --write . --config prettier.config.ts",
    "preinstall": "npx only-allow pnpm",
    "lint": "pnpm lint:stylecheck && pnpm lint:typecheck && pnpm lint:eslint",
    "lint:eslint": "pnpm dlx eslint .",
    "lint:stylecheck": "prettier --check . --config prettier.config.ts",
    "lint:typecheck": "tsc",
    "publish:major": "pnpm version major && git push --follow-tags",
    "publish:minor": "pnpm version minor && git push --follow-tags",
    "publish:patch": "pnpm version patch && git push --follow-tags",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006"
  },
  "dependencies": {
    "@fontsource/arimo": "^5.2.6",
    "@fontsource/bungee": "^5.2.6"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.31.0",
    "@mui/material": "^7.2.0",
    "@storybook/addon-docs": "9.1.0-alpha.8",
    "@storybook/addon-themes": "9.1.0-alpha.8",
    "@storybook/react-vite": "9.1.0-alpha.8",
    "@svgr/rollup": "^8.1.0",
    "@tanstack/react-form": "^1.14.1",
    "@tolgee/format-icu": "^6.2.6",
    "@tolgee/react": "^6.2.6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "9.1.0-alpha.8",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.18",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "9.1.0-alpha.8",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.5"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.0",
    "@tanstack/react-form": "^1.14.0",
    "@tolgee/format-icu": "^6.2.0",
    "@tolgee/react": "^6.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=23.11.0",
    "npm": ">=10.9.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
